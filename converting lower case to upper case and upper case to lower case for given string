#include <stdio.h>
#include<string.h>
int main() {
    // Write C code here
    char str[100];
    int i;
        scanf("%[^\n]s",str);
        for(i=0;str[i]!='\0';i++)
        {
            if(str[i]>=65 && str[i]<=90)
            {
                str[i]=str[i]+32;
            }
        else if  (str[i]>=97 && str[i]<=112)
        {
            str[i]=str[i]-32;
        }
        }
        printf("%s",str);
    return 0;
}
